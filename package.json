{
  "version": "0.3.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup src/index.tsx --watch",
    "test": "jest -i",
