    "prepare": "npm run build"
  },
  "peerDependencies": {
    "effector": "^22",
    "react": "^17 || ^18"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^17 || ^18",
    "effector": "^22.1.2",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.3",
    "ts-jest": "^28.0.1",
    "tslib": "^2.4.0",
    "tsup": "^6.2.0",
    "typescript": "^4.5.4"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "effector-factorio",
  "author": "Anton Kosykh",
  "repository": {
    "type": "git",
    "url": "https://github.com/kelin2025/effector-factorio"
